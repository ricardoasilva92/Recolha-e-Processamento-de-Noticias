    function verificaUserAgent() {
        var user_agent  = navigator.userAgent.toLowerCase();
        var urlAtual    = location.href;
        var screenWidth = screen.width;

        if(user_agent.match(/Android/i) || user_agent.match(/Windows Phone/i) || user_agent.match(/iPhone/i) || screenWidth <= 728) {
            if(window.location.protocol != 'https:') {
                urlAtual = location.href.replace("http://", "https://");
            }
            window.location = urlAtual.replace("://", "://m.");
        }
    }

    verificaUserAgent();
