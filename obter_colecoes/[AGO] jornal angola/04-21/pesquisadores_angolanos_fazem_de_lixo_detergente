   if (document.mmm_fo) document.write ("&;mmm_fo=1");
   document.write ("'><\/scr"+"ipt>");
//]]>--></script><noscript><a href='http://pub.sapo.pt/ck.php?n=a477847e&cb=544580720' target='_blank'><img src='http://pub.sapo.pt/avw.php?zoneid=4182&cb=544580720&n=a477847e' border='0' alt='' /></a></noscript>
