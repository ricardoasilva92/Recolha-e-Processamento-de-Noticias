                    className: 'sideMenuIsExpanded',
                    ignore: '.sideNavigation',
                    scrollTarget: $header,
                    targetSelector: '.js-sideNavigation'
                });

                // Show the menu trigger
                $menuTrigger.show();
                
                // Load Header Parts
                VisibilityChecker.start($parts, {
                    onVisible: function($el) {
                        this.stop();

                        ContentLoader.start($el, {
                            cache: true
                        });
                    }
                });
            });
