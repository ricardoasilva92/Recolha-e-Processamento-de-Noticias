  var top=(screen.height-125)/2;

	var left=(screen.width-200)/2;

	function popup(id,lang){

	window.open("spip.php?page=envoiami&recalcul=oui&id_article="+id+"&lang="+lang,"_blank", "top="+top+",left="+left+",scrollbars=no,width=200,height=125");
	}

	function verifierForm()
	{
		if(document.formforum.pseudoReagir.value== "")
		{
			return false;
		}
		if(document.formforum.commentaireReagir.value== "")
		{
			return false;
		}
		if(document.formforum.titreReagir.value== "")
		{
			return false;
		}
		
		return true;
		
	}

</script>

