						<ul class="cp-result-table__list">

							{{#each homeMatchesDetail}}
								<li class="cp-result-table__list__item"><span class="cp-result-table__list__name">{{homeTeamName}} </span><span class="cp-result-table__list__result">{{homeScore}} - {{awayScore}}</span><span class="cp-result-table__list__name">{{awayTeamName}} </span></li>
							{{/each}}

						</ul>
						
					</div>

					<div class="cp-result-table__game__item">
						<p class="cp-result-table__number">{{awayTotalMatches}} <span class="cp-result-table__local">Como visitante</span></p>
						<div class="cp-result-table__progress">
							<div class="cp-result-table__progress__value" style="width: {{awayWinsPercentage}}%;">v</div>
							<div class="cp-result-table__progress__value-black" style="width: {{awayDrawsPercentage}}%;">E</div>
							<div style="width: {{awayLossesPercentage}}%;">D</div>
						</div>

						<ul class="cp-result-table__list">

							{{#each awayMatchesDetail}}
								<li class="cp-result-table__list__item"><span class="cp-result-table__list__name">{{homeTeamName}} </span><span class="cp-result-table__list__result">{{homeScore}} - {{awayScore}}</span><span class="cp-result-table__list__name">{{awayTeamName}} </span></li>
							{{/each}}

						</ul>

					</div>

					

				</div>

			</div>


      </script>

